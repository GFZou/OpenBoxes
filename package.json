    "verbose": true,
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/js/tests/__mocks__/styleMock.js"
    },
    "testURL": "http://localhost/"
  },
