  "main": "index.js",
  "engines": {
    "node": "^14",
    "npm": "^6 || ^7"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "watch": "webpack --watch --colors --progress --mode=development",
    "bundle": "webpack --mode=production",
    "test": "jest",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "eslint": "eslint --fix --ext .js,.jsx ./src/js/"
  },
